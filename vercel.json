{
  "version": 2,
  "builds": [
    {
      "src": "app.js", 
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "10mb" 
      }
    },
    {
      "src": "build",
      "use": "@vercel/static" 
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      }
    }
  ]
}
